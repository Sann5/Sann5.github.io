	month = jan,
	year = {2021},
	doi = {10.12688/f1000research.29032.1},
	note = {Type: article},
	keywords = {adaptability, data analysis, reproducibility, scalability, sustainability, transparency, workflow management},
	file = {Full Text PDF:/Users/santiago/Zotero/storage/75NKI5LC/Mölder et al. - 2021 - Sustainable data analysis with Snakemake.pdf:application/pdf},
